{
	"$categoryId": {
		"prefix": "$c.id",
		"body": [
			"${category.id?c}"
		],
		"description": "${category.id?c}",
		"scope": "text.html.ftl"
	},
	"$categoryName": {
		"prefix": "$c.name",
		"body": [
			"${category.name!}"
		],
		"description": "${category.name!}",
		"scope": "text.html.ftl"
	},
	"$categorySlug": {
		"prefix": "$c.slug",
		"body": [
			"${category.slug!}"
		],
		"description": "${category.slug!}",
		"scope": "text.html.ftl"
	},
	"$categoryFullPath": {
