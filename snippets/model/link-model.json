{
    "$linkId": {
		"prefix": "$link.id",
		"body": [
            "${link.id?c}"
		],
		"description": "$linkId",
		"scope": "text.html.ftl"
    },
    "$linkName": {
		"prefix": "$link.name",
		"body": [
            "${link.name!}"
		],
		"description": "$linkName",
		"scope": "text.html.ftl"
    },
    "$linkUrl": {
		"prefix": "$link.url",
		"body": [
            "${link.url!}"
		],
		"description": "$linkUrl",
		"scope": "text.html.ftl"
    },
    "$linkLogo": {
		"prefix": "$link.logo",
		"body": [
            "${link.logo!}"
		],
		"description": "$linkLogo",
		"scope": "text.html.ftl"
    },
    "$linkDescription": {
		"prefix": "$link.description",
		"body": [
            "${link.description!}"
		],
		"description": "$linkDescription",
		"scope": "text.html.ftl"
    },
    "$linkTeam": {
		"prefix": "$link.team",
		"body": [
            "${link.team!}"
		],
		"description": "$linkTeam",
		"scope": "text.html.ftl"
