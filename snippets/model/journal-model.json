{
    "$journalId": {
		"prefix": "$j.id",
		"body": [
            "${journal.id?c}"
		],
		"description": "${journal.id?c}",
		"scope": "text.html.ftl"
    },
    "$journalContent": {
		"prefix": "$j.content",
		"body": [
            "${journal.content!}"
		],
		"description": "${journal.content!}",
		"scope": "text.html.ftl"
    },
    "$journalLikes": {
		"prefix": "$j.likes",
		"body": [
            "${journal.likes?c}"
		],
		"description": "${journal.likes?c}",
		"scope": "text.html.ftl"
    },
    "$journalCreateTime": {
		"prefix": "$j.createTime",
		"body": [
            "${journal.createTime?string('yyyy-MM-dd')}"
		],
		"description": "${journal.createTime?string('yyyy-MM-dd')}",
		"scope": "text.html.ftl"
    },
    "$journalUpdateTime": {
		"prefix": "$j.updateTime",
		"body": [
            "${journal.updateTime?string('yyyy-MM-dd')}"
		],
		"description": "${journal.updateTime?string('yyyy-MM-dd')}",
		"scope": "text.html.ftl"
    }
}