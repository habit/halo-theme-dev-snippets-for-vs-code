{
    "$tagId": {
		"prefix": "$t.id",
		"body": [
            "${tag.id?c}"
		],
		"description": "${tag.id?c}",
		"scope": "text.html.ftl"
    },
    "$tagName": {
		"prefix": "$t.name",
		"body": [
            "${tag.name!}"
		],
		"description": "${tag.name!}",
		"scope": "text.html.ftl"
    },
    "$tagSlug": {
		"prefix": "$t.slug",
		"body": [
            "${tag.slug!}"
		],
		"description": "${tag.slug!}",
		"scope": "text.html.ftl"
	},
	"$tagFullPath": {
		"prefix": "$t.fullPath",
		"body": [
            "${tag.fullPath!}"
		],
		"description": "${tag.fullPath!}",
		"scope": "text.html.ftl"
    },
    "$tagThumbnail": {
		"prefix": "$t.thumbnail",
		"body": [
            "${tag.thumbnail!}"
		],
		"description": "${tag.thumbnail!}",
		"scope": "text.html.ftl"
    },
    "$tagCreateTime": {
		"prefix": "$t.createTime",
		"body": [
            "${tag.createTime?string('yyyy-MM-dd')}"
		],
		"description": "${tag.createTime?string('yyyy-MM-dd')}",
		"scope": "text.html.ftl"
    },
    "$tagUpdateTime": {
		"prefix": "$t.updateTime",
		"body": [
            "${tag.updateTime?string('yyyy-MM-dd')}"
		],
		"description": "${tag.updateTime?string('yyyy-MM-dd')}",
		"scope": "text.html.ftl"
    }
}